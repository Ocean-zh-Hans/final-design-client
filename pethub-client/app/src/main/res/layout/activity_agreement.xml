<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/agreementRootConstraintLayout"
    android:background="@color/background_normal"
    tools:context=".ui.activity.AgreementActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/agreementToolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:navigationIcon="@drawable/ic_back_left_arrow" />

    <WebView
        android:id="@+id/agreementContentWebView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_margin="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/agreementToolbar" />

</androidx.constraintlayout.widget.ConstraintLayout>